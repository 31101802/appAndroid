<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuScreen"
    tools:ignore="MergeRootFrame"
    android:background="@drawable/background">
    <ImageView
        android:layout_width="1012dp"
        android:layout_height="323dp"
        android:id="@+id/imageView5"
        android:layout_gravity="center_horizontal|top"
        android:src="@drawable/backgroundHead"
        android:layout_marginTop="-107dp" />
<ImageView
    android:layout_width="244dp"
    android:layout_height="204dp"
    android:id="@+id/imageView"
    android:layout_gravity="center_horizontal"
    android:src="@drawable/logo"
    android:contentDescription="@string/description_logo" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:layout_gravity="center_horizontal|bottom"
        android:src="@drawable/twitter_icon"
        android:layout_marginBottom="15dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:layout_gravity="center_horizontal|bottom"
        android:src="@drawable/vimeo_icon"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="70dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:layout_gravity="center_horizontal|bottom"
        android:src="@drawable/facebook_icon"
        android:layout_marginBottom="15dp"
        android:layout_marginRight="70dp" />

    <Button
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="Ver Fotos"
        android:id="@+id/btnViewPhotos"
        android:layout_gravity="center"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:background="#db451a"
        android:onClick="onImageGridClick"
        android:layout_marginTop="20dp" />

    <Button
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:text="Subir Fotos"
        android:id="@+id/btnUpload"
        android:layout_gravity="center"
        android:textColor="#ffffff"
        android:background="#da3d11"
        android:textStyle="bold"
        android:layout_marginTop="70dp" />

</FrameLayout>